// parameterized constructor. 
#include<iostream>
using namespace std;
class area{
	public:
		area(int length,int breath)
		{
			cout<<"Area of rectangle : "<<length*breath;
		}
};
int main()
{
	cout<<"I am chirag sanchaniya "<<endl;
	cout<<"En_no    220130318030 "<<endl;
	cout<<"---------------------"<<endl;
	area(3,8);
}
