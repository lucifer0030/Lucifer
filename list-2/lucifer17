#include<iostream>
using namespace std;

enum boolean{f,t};
enum days{sun,mon,thu,wen,thr,fri,sat};
int main()
{
	// enum data type 
	cout<<"I am chirag sanchaniya "<<endl;
	cout<<"En_no    220130318030 "<<endl;
	cout<<"---------------------"<<endl;
	cout<<"Enter the false or true : ";
	cout<<"False  "<<f<<endl;
	cout<<"True   "<<t<<endl;
	
	cout<<"Sunday      : "<<sun<<endl;
	cout<<"Monday      : "<<mon<<endl;
	cout<<"Thuday      : "<<thu<<endl;
	cout<<"Wednesday   : "<<wen<<endl;
	cout<<"Thresday    : "<<thr<<endl;
	cout<<"Friday      : "<<fri<<endl;
	cout<<"Satrday     : "<<sat<<endl;
	return 0;
}
