#include<iostream>
using namespace std;
main()
{
	int n;
	cout<<"I am chirag sanchaniya "<<endl;
	cout<<"En_no    220130318030 "<<endl;
	cout<<"---------------------"<<endl;
	cout<<"Enter the number : ";
	cin>>n;
	
	if(n%2==0)
	{
		cout<<"This is Even number ";
	}
	else
	{
		cout<<"This is odd number ";
	}
	return 0;
}
